<!DOCTYPE html>
<html lang="en" ng-app = "app">
<head>
  <meta charset="UTF-8">
  <title>Angular app</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- Normalize -->
  <link rel="stylesheet" href="/assets/libs/bower/normalize-css/normalize.css">
  <!-- Custom -->
  <link rel="stylesheet" href="/assets/css/main.css">
</head>

<body>
  <div class="row">
    <div class = "medium-6 medium-offset-3 columns">
      <h3>Welcome to the Angular happy place.</h3>
      <label for="">Try it out:</label>
      <input type="text" ng-model = "test" value = "Everything's working" >
      {{test}}
    </div>

  </div>

  <script src="/assets/libs/bower/angular/angular.js"></script>
  <script src="/assets/libs/bower/angular-route/angular-route.js"></script>

  <script src="app.module.js"></script>
</body>
</html>
